__fn macro_HPSPreg $agent $HP_or_SP $amount{
	/* "Var1 = Obj 1~
	Var2 = 0/1# HP/MP
	Var3 = Amount"*/
	//v[341] = $HP_or_SP * 2
	v[342] = $agent * 300 + 4806 + ($HP_or_SP * 2)
	//v[343] = v[342] - 1
	//v[v[342]] += $amount
	// "Min(HealedHP,MaxHP)"
