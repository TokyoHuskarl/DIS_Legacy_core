				func_set_tileflag_in_square_range(flag_pikecovered ShieldWall_Cover_SquareSide ShieldWall_Cover_Diff Temp11 Temp13)
			}
		}

	    //Set tile instants end

	    // "++"
	    func_main_save_agent_vars()
	    v[536] += 1
	    
	}

	@wait 0
	// "Workers?"
	v[2941].copy v[2945], 4
