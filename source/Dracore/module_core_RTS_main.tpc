}

__fn macro_cord_diff $x $y {
	    
	    //v[__id($x)] = ($x / 10000 + v[53] - v[403])/16
	    //v[__id($y)] = ($y / 10000 + v[54] - v[404])/16
	    
	    //this one was far faster
	    v[__id($x)..__id($y)]/=10000
	    v[__id($x)] .sub v[403], 2
	    v[__id($x)] .add v[53], 2
	    v[__id($x)..__id($y)]/=16
}



