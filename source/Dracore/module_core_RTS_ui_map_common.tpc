__fn show_Ally_AR_HP_bar $showwhat {
                //get HP/AR ratio
                v[v[303]].copy v[303], 2
                v[304] -= 16
                v[341] = 100 - v[v[302] + 260] * 22
                v[342] = 100
                v[735] = v[(v[v[301]]*300+4735)]
                //If selected units has Cohort assignment
                @if `between(v[735],1,10){
                v[303] -=4
                v[706] = ((v[706] * 100 / (max(v[705], 1) * 5) * 10)-10+(400 - 200*$showwhat)) + v[735]
                 @pic[v[310]].show {
                    "ui\cohortHPbar"
                    .pos v[303], v[304] .center
                    .scrollWithMap
                    .chromakey 1
                    .scale 100
                    .rgbs v[342], v[342], v[342], v[341]
                    .trans 0
                    .grid 10, 40 .cell v[706]
                    .mapLayer 7
                    .eraseWhenTransfer
                    .affectedByFlash
                    .affectedByShake
                }   
                }.else bl{
                
                v[706] = max(v[706] * 100 / (max(v[705], 1) * 5) * 3, 3) -$showwhat
                
                @pic[v[310]].show {
                    "ui\HP_bar_united"
                    .pos v[303], v[304] .center
                    .scrollWithMap
                    .chromakey 0
                    .scale 100
                    .trans 0
                    .rgbs v[342], v[342], v[342], v[341]
                    .grid 3, 20 .cell v[706]
                    .mapLayer 7
                    .eraseWhenTransfer
                    .affectedByFlash
                    .affectedByShake
                }   
                }
}

