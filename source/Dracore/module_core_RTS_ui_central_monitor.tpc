__fn func_ui_show_terrain_info {
       @if s[18] .isOn() {
            @if s[5] .isOff() {
                //// "#Check Terrain"
                v[51].copy var1, 2
                var2 -= 8
                //@call .cev 2000
		macro_convert_relativeXY_into_maptileXY(var1,var2,reg1,reg2)
                @if `v[1974] != reg1 || v[1975] != reg2 {
                    //// "######################Get Terrain and Elevation info"
                    v[313] = v[v[1182] + reg1 - v[69] + (reg2 - v[70]) * v[2061]]
                    v[341] = v[313] / 100000000
                    v[350] = v[51]
                    v[351] = v[52] - 16
                    v[342] = v[1239] +v[313] % 100

                    @if .testPlay() {
                        t[501] .asg "

    \t[20110] \t[\v[342]] \t[20111]\v[341] (\v[21],\v[22])" .extract
                        
                    } .else bl {
                        t[501] .asg "
    \t[20110] \t[\v[342]] \t[20111]\v[341]" .extract
                        
                    }

                    //// "######################"
                    v[1974] = reg1
                    v[1975] = reg2
                    @pic[PicID_Unit_SP].strpic {
                        t[501]
                        .pos v[1296], v[1297] .topLeft
                        .size 0, 0                        .chromakey 1
                        .scale 100
                        .trans 0
                        .rgbs 100, 100, 100, 100
                        .font t[530], 11 .noShadow
                        .spacing 0, 0
                        .skin "" .nobg .noframe .noGradation .noPadding
                        .mapLayer 8
                        .eraseWhenTransfer
                        .affectedByFlash
                        .affectedByShake
                    }
                    
                }
                
            } .else bl {
                @pic[PicID_Unit_SP].erase
                
            }
            
        }
}


