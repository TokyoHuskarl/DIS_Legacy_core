cev .id(2031), .name("cev2031"), {
@loop 800 .dst v[341] {
    v[342] = v[1270] + v[1271] * v[341]
    @if v[v[342]] == v[11] {
        v[v[342] + 1] += v[12]
        @break
        
    }
    @if v[v[342]] == 0 {
        v[v[342]] = v[11]
        v[v[342] + 1] += v[12]
        s[150].on
        v[351] = 200 + v[11]
        @call .cev v[351]
        v[v[342] + 5] = v[702]
        s[150].off
        @break
        
    }
    
}

}