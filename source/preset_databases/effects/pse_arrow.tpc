			    @if `v[v[342] - 123] != 100 {
				v[v[342] + 32] = v[v[342]] > 0 ? 4 : 0
				// "#Shield Motion"
				v[342] = Ptr10 
				v[342] += 4943
				@if v[v[342]] != 1 {
				    v[v[342]] = 2
				    v[342] -= 215
				    v[v[342]] += v[v[342]] < 300 ? 900 : 0
				    
				}
				
			    }
			    @if s[307] .isOff() {
				@if `v[183] < v[4572] / 2 {
				    // "#描画範囲内か？"
				    v[220] = ((v[220]+1)%30)+1201
				    //9.3.23
				    //v[220] += 1201
				    //v[220] += 1
				    v[223] = v[220]
				    v[221] += rnd(-6, 6)
				    v[222] += rnd(-3, 3)
				    v[222] += s[162] == 1 ? 10 : 0
				    // "#サイズは最低100%"
