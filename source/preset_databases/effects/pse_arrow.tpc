			v[421] = v[342] >= v[341] ? v[343] : 100
			
		    } .else bl {
			v[421] = 100
			
		    }
		    // "#Elevation Bonus"
		    v[421] += 10 * v[320]
		    /* "＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃
	ここそのうち用意した関数に置き換える
	＃＃＃＃＃＃＃＃＃＃＃＃＃＃＃"*/
		    // "ADを取得"
		    /*v[11] = v[299]
		    v[12] = 1
		    @call .cev 2004*/
		    macro_get_agent_slot_to_r1(v[299] SLOT_CURRENT_AD)
		    v[301] = v[21]
		    // "ARかMRか取得"
		    v[722] = Ptr9
		    v[722] += 4822
		    v[302] = v[v[722]]
		    // "射撃属性"
		    v[303] = 1
		    // "AABits"
		    v[19] = v[722] + 2
		    v[19] = v[v[19]]
		    //@call .cev 40
		    func_damage_calculation()
		    // "############"
		    // "Effect"
		    // "#あたったので演出チェック"
		    @if `s[v[300] + 500] == 1 {
			@if v[21] == v[22] {
			    // "Red Flash"
			    v[376] += v[182] < 5 ? 1 : 0
			    @if v[376] >= 2 {
				v[182] += 1
				v[329] = rnd(0, 2)
				// "#########################"
				v[472] = divmul(60, 100, v[2216])
				v[473] = rnd(90, 110)
				v[474] = divmul(v[607] + v[1001], v[1281], 50)
				t[510] .asg "arrow_hit_body_"
				t[510] .cat v[329]
